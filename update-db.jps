type: install
name: Anti-virus
baseUrl: https://raw.githubusercontent.com/panslothda/Jelastic_Addon_AV-Scanner/master/
version: 0.1
logo: clamav-logo.png

onInstall:
  set:
    result: a
  cmd [*]:
    - freshclam > ${this.result}
  user: root
  sayYes: true

success:
  text:  Clamav Database has been updated ${this.result} test

